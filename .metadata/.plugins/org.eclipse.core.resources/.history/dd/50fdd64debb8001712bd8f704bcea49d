package garage;

/**
 * @author Nick
 *
 */
public class Moteur {
	
	private TypeMoteur type;
	private String cylindre;
	private double prix;
	
	
	
	public Moteur(String cylindre, double prix) {
		super();
		this.cylindre = cylindre;
		this.prix = prix;
	}



	public Moteur(TypeMoteur type, String cylindre, double prix) {
		super();
		this.type = type;
		this.cylindre = cylindre;
		this.prix = prix;
	}
	
	
	
	
	

}
